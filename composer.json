{
    "name": "adamibrahim/superauth",
    "type": "library",
    "description": "Laravel 5.6 Authentication Package",
    "keywords": [
        "Laravel 5.6",
        "package",
        "Admin",
        "Roles",
        "Email Confirmation"

    ],
    "homepage": "https://github.com/adamibrahim/superauth",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Ibrahim",
            "email": "adam@hostato.com",
            "homepage": "https://www.hostato.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Adam\\Superauth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adam\\Superauth\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adam\\Superauth\\SuperauthServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
